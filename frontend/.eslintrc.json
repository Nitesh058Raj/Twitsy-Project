{
  "extends": ["next/core-web-vitals", "prettier"],
  "plugins": [
    "unused-imports",
    "prettier"
  ],
  "rules": {
    "no-unused-vars": "off",
    "unused-imports/no-unused-imports": "error",
    "prettier/prettier": ["error"],
    "react/jsx-pascal-case": ["error", {
      "ignore": []
    }]
  }
}
